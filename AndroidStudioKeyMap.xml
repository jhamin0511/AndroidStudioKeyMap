<keymap version="1" name="jhm keymap" parent="$default">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
  </action>
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="ctrl x" />
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="ctrl v" />
  </action>
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
  </action>
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id="ActivateGitHubPullRequestsToolWindow">
    <keyboard-shortcut first-keystroke="alt 8" />
  </action>
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="Android.SyncProject">
    <keyboard-shortcut first-keystroke="ctrl alt r" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="shift ctrl alt f4" />
  </action>
  <action id="Compare.SameVersion">
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
  <action id="Find">
    <keyboard-shortcut first-keystroke="ctrl f" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="Git.CompareWithBranch">
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="GotoDeclaration">
    <mouse-shortcut keystroke="control button1" />
  </action>
  <action id="GotoImplementation">
    <mouse-shortcut keystroke="control alt button1" />
  </action>
  <action id="GotoNextError">
    <keyboard-shortcut first-keystroke="f2" />
    <mouse-shortcut keystroke="button4" />
  </action>
  <action id="GotoPreviousError">
    <keyboard-shortcut first-keystroke="shift f2" />
    <mouse-shortcut keystroke="button5" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="alt period" />
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="alt comma" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="shift ctrl alt r" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="UpdateRunningApplication" />
  <action id="Vcs.ShowDiffWithLocal">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
</keymap>
